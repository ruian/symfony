{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}

    {% set dumps_count = collector.dumps|length %}

    {% set icon %}
        <img alt="debug()" src="data:image/png;base64," />
        <span class="sf-toolbar-status{% if dumps_count > 0 %} sf-toolbar-status-yellow{% endif %}">{{ dumps_count }}</span>
    {% endset %}
    {% if dumps_count %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>debug()</b>
            <ul>
                {% for dump in collector.dumps %}
                <li>
                in {% if dump.file %}<abbr title="{{ dump.file }}">{{ dump.name }}</abbr>{% else %}{{ dump.name }}{% endif %}
                on line {{ dump.line }}
                </li>
                {% endfor %}
            </ul>
        </div>
    {% endset %}
    {% endif %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': true } %}
{% endblock %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon">{% spaceless %}
        <img alt="debug()" src="data:image/png;base64," />
            {% endspaceless %}</span>
        <strong>debug()</strong>
        <span class="count">
            <span>{{ collector.dumps|length }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>debug()</h2>

    <link rel="stylesheet" href="{{ asset('bundles/webprofiler/css/var_debug.css') }}" />
    <script src="{{ asset('bundles/webprofiler/js/var_debug.js') }}"></script>

    <ul id="var_debug" class="alt">
        {% for dump in collector.dumps %}
        <li class="dump sf-reset">
            in {% if dump.file %}<abbr title="{{ dump.file }}">{{ dump.name }}</abbr>{% else %}{{ dump.name }}{% endif %}
            on line {{ dump.line }}:
            <a onclick="arrayToggle(this)"> âŠž </a><span class="key"></span>
            <span class="array-compact">
            {% if dump.excerpt %}{{ dump.excerpt|raw }}{% else %}{{ dump.file|file_excerpt(dump.line) }}{% endif %}
            </span>

            <pre class="event"><script>document.write(htmlizeEvent({{ dump.json|raw }}))</script></pre>
        </li>
        {% endfor %}
    </ul>
{% endblock %}
